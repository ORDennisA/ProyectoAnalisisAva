<li class="nav-item">
    <a href="@Url.Action("FormularioPrestamo","Home")" class="btn btn-primary">Agregar Prestamo</a>
</li>
<li class="nav-item">
    <a href="@Url.Action("EditarPrestamo","Home")" class="btn btn-primary">Borrar/Editar</a>
</li>

<div class="col-sm-8"><h2>Prestamo </h2></div>
<table class="table table-condensed">
    <tr>
        <th>Id_Prestamo</th>
        <th>Fecha_de_Salida</th>
        <th>Fecha_Maxima_de_Devolucion</th>
        <th>Fecha_de_Devolucion</th>
        <th>Id_Usuarios</th>
        <th>Id_Tipo_de_Prestamos</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td> @item.id_Prestamo</td>
            <td> @item.Fecha_de_Salida</td>
            <td> @item.Fecha_Maxima_de_Devolucion</td>
            <td> @item.Fecha_de_Devolucion</td>
            <td> @item.Id_Usuarios</td>
            <td> @item.Id_Tipo_de_Prestamos</td>
        </tr>
    }
</table>