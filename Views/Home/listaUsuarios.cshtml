
<li class="nav-item">
    <a href="@Url.Action("FormularioUsuarios","Home")" class="btn btn-primary">Agregar Usuarios</a>
</li>
<li class="nav-item">
    <a href="@Url.Action("EditarUsuarios","Home")" class="btn btn-danger">Borrar/Editar</a>
</li>

<div class="col-sm-8"><h2>Usuarios</h2></div>
<table class="table table-condensed">
    <tr>
        <th>Id_Usuarios</th>
        <th>Nombres</th>
        <th>Apellidos</th>
        <th>DNI</th>
        <th>Domicilio</th>
        <th>Fecha_de_nacimiento</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td> @item.id_Usuarios</td>
            <td> @item.Nombres</td>
            <td> @item.Apellidos</td>
            <td> @item.DNI</td>
            <td> @item.Domicilio</td>
            <td> @item.Fecha_de_nacimiento</td>
        </tr>
    }
</table>
